#solved By Md Ariful Islam

def calculator(num1, operator, num2):
    if operator=='+':
        return round(num2+num1)
    elif operator=='-':
        return round(num1-num2)
    elif operator=='*':
        return round(num2*num1)
    else:
        if num2==0:
            return "Can't divide by 0!"
        else:
            return round(num2/num1)

